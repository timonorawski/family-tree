# Cloudflare tunnel for the readonly public site
CLOUDFLARE_TUNNEL_TOKEN=your-tunnel-token-here

# Cloudflare tunnel for the editable site (behind Zero Trust)
CLOUDFLARE_TUNNEL_TOKEN_EDIT=your-edit-tunnel-token-here

# Static build mode
VITE_STATIC=false
NODE_ADAPTER=false

# Encryption (for encrypted static builds)
STATIC_CRYPT_SECRET=your-32-byte-hex-secret-here
STATIC_CRYPT_TIERS=family,extended
STATIC_CRYPT_BASE_URL=https://family.example.com/
